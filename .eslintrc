{
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": ["react"],
	"env": {
		"browser": true,
		"jasmine": true,
		"jest": true
	},
	"rules": {
		"semi": ["error", "never"],
		"comma-dangle": ["error", "always-multiline"]
	},
	"settings": {
		"react": {
			"pragma": "React",
			"version": "detect"
		}
	}
}
